[Unit]
Description=registrar server daemon
Conflicts=getty@tty1.service

[Service]
ExecStart=/system/servers/registrar
StandardInput=tty-force
StandardOutput=append:/var/log/registrar.log
StandardError=append:/var/log/registrar.log
Restart=no
RestartSec=500ms
StartLimitBurst=5

[Install]
WantedBy=graphical.target
